<!-- Import Google Icon Font -->
<style>
    @font-face {
      font-family: 'Material Icons';
      font-style: normal;
      font-weight: 400;
      src: url(<%= staticServer %>/static/materialize/img/iconfont/MaterialIcons-Regular.eot); /* For IE6-8 */
      src: local('Material Icons'),
             local('MaterialIcons-Regular'),
             url(<%= staticServer %>/static/materialize/img/iconfont/MaterialIcons-Regular.woff2) format('woff2'),
             url(<%= staticServer %>/static/materialize/img/iconfont/MaterialIcons-Regular.woff) format('woff'),
             url(<%= staticServer %>/static/materialize/img/iconfont/MaterialIcons-Regular.ttf) format('truetype');
    }

    .material-icons {
      font-family: 'Material Icons';
      font-weight: normal;
      font-style: normal;
      font-size: 24px;
      line-height: 1;
      letter-spacing: normal;
      text-transform: none;
      display: inline-block;
      white-space: nowrap;
      word-wrap: normal;
      direction: ltr;

      /* Support for all WebKit browsers. */
      -webkit-font-smoothing: antialiased;
      /* Support for Safari and Chrome. */
      text-rendering: optimizeLegibility;

      /* Support for Firefox. */
      -moz-osx-font-smoothing: grayscale;

      /* Support for IE. */
      font-feature-settings: 'liga';
    }

    #site-header {
      height: 100px;
      background-image: url(<%= staticServer %>/static/img/black_header.jpg);
      background-repeat: repeat;
    }

    section.first-block a {
      background-image: url(<%= staticServer %>/static/img/dice.jpg);
      background-size: 300px;
      background-position: 0 -20px;

      display: table;
      position: absolute;
      top: 126px;
      left: 30px;

      width: 300px;
      height: 100px;
    }

    #fifth-block {
      background-image: url(<%= staticServer %>/static/img/fifth_block.jpg);
      background-size: 100%;
      border: 1px solid #212121;
      padding: 20px;
    }

    div.vt-footer {
      background-image: url(<%= staticServer %>/static/img/space.gif);
      background-size: 300px 300px;
      background-position: 0px -200px;
      background-repeat: repeat;
    }
</style>

<!--Import materialize.css-->
<link type="text/css" rel="stylesheet" href="<%= staticServer %>/static/materialize/css/materialize.min.css"  media="screen,projection"/>

<!--Let browser know website is optimized for mobile-->
<meta name="viewport" content="width=device-width, initial-scale=1.0"/>

<!--Importar CSS do projeto-->
<link href="<%= staticServer %>/static/css/style.css" rel="stylesheet">